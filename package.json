    "watchRollup": "./node_modules/rollup/bin/rollup -c --watch"
  },
  "author": "RAVENLORD",
  "license": "BEST_LICENSE",
  "private": true,
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "phaser": "2.6.2"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-2": "6.13.0",
    "eslint": "3.6.0",
    "http-server": "0.9.0",
    "rollup": "0.36.0",
    "rollup-plugin-commonjs": "5.0.4",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-watch": "2.5.0"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-2"
